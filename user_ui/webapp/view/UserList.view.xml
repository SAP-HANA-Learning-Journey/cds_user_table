<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="user_ui.user_ui.controller.UserList" displayBlock="true">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
					    <sap.ui.layout.form:SimpleForm xmlns:sap.ui.layout.form="sap.ui.layout.form" xmlns:sap.ui.core="sap.ui.core" editable="true" layout="ResponsiveGridLayout" id="form0" xmlns:cd="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" cd:w5g.dt.context="/Users">
					        <sap.ui.layout.form:content>
					            <sap.ui.core:Title text="{i18n>ttlUserEntry}" id="title0"/>
					            <Label text="{i18n>lblUserId}" id="label0"/>
					            <Input width="100%" id="input0"/>
					            <Label text="{i18n>lblFirstName}" id="label1"/>
					            <Input width="100%" id="input2"/>
					            <Label text="{i18n>lblLastName}" width="100%" id="label2"/>
					            <Input xmlns="sap.m" id="input3"/>
					            <Label text="{i18n>lblEmail}" width="100%" id="label3"/>
					            <Input xmlns="sap.m" id="input4"/></sap.ui.layout.form:content>
					    </sap.ui.layout.form:SimpleForm>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>