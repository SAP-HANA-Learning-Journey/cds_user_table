{
  "welcomeFile": "/user_ui/index.html",
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
  	{
  		"source": "(*.)(xsodata)",
  		"destination": "user_js_be",
  		"authenticationType": "xsuaa"
  	},
    {
      "source": "^/user_ui/(.*)$",
      "target": "$1",
      "localDir": "webapp"
    }
  ]
}